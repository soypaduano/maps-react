{
    "name": "maps-music-start",
    "version": "1.0.0",
    "description": "execute nested scripts",
    "main": "",
    "scripts": {
      "frontend": "cd frontend && npm run start",
      "backend": "cd  backend && npm run start",
      "tab:frontend": "ttab -t 'Frontend' npm run frontend",
      "tab:backend": "ttab -t 'Backend' npm run backend",
      "all": "run-s tab:frontend tab:backend"
    },
